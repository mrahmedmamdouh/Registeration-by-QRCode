<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <ImageView
        android:layout_width="150dp"
        android:layout_height="192dp"
        android:background="@drawable/portrait"
        android:layout_marginTop="75dp"
        android:layout_marginLeft="750dp" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="رقم التذكرة"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="400dp"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="220"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="100dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:id="@+id/txt_TicketNumber"
        android:gravity="right"/>

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="الاسم"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="400dp"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="اسم مالك التذكرة"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="100dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:id="@+id/txt_TicketOwner"
        android:gravity="right"/>

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="تاريخ الصلاحية"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="400dp"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="30/09/2019"
        android:layout_marginTop="220dp"
        android:layout_marginLeft="100dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:id="@+id/txt_ValidityDate"
        android:gravity="right"/>

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="الدرجة"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="400dp"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="كبار الزوار"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="100dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:id="@+id/txt_Class"/>

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="البوابة المصرح الدخول منها"
        android:layout_marginTop="360dp"
        android:layout_marginLeft="400dp"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:text="بوابة رقم 3"
        android:layout_marginTop="360dp"
        android:layout_marginLeft="100dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:id="@+id/txt_GateNumber"/>

    <ImageView
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="@drawable/valid"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="777dp"
        android:visibility="gone"
        android:id="@+id/img_TicketValidityStatus"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="تذكرة صالحة"
        android:layout_marginTop="390dp"
        android:layout_marginLeft="770dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:textColor="@android:color/holo_green_dark"
        android:visibility="gone"
        android:id="@+id/txt_ValidTicket"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="تذكرة غير صالحة"
        android:layout_marginTop="390dp"
        android:layout_marginLeft="750dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:textColor="@android:color/holo_red_dark"
        android:visibility="gone"
        android:id="@+id/txt_InvalidTicket"/>

    <com.google.android.material.button.MaterialButton
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:text="@string/back"
        android:layout_marginTop="400dp"
        android:layout_marginLeft="20dp"
        android:onClick="onDismissScreen"/>

</RelativeLayout>